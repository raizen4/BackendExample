{
  "name": "listing-ui-web",
  "version": "1.0.0",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "yarn client:prod && yarn server:prod",
    "pipeline:listing-ui-web": "yarn client:prod && yarn server:prod",
    "dev": "webpack-dev-server --progress --inline --config webpack.client.dev.js",
    "client:prod": "webpack --config webpack.client.prod.js",
    "server:prod": "webpack --config webpack.server.prod.js",
    "start": "node -r dotenv/config dist/server/app.js",
    "ssr": "rimraf dist && yarn build && cross-env NODE_ENV=production yarn start",
    "cy:open": "cypress open",
    "cy:dev": "start-server-and-test dev http-get://localhost:3000 cy:open",
    "cy:headless": "start-server-and-test dev http-get://localhost:3000 cy:run",
    "cy:verify": "cypress verify",
    "cy:run": "cypress run",
    "e2e": "yarn cy:verify & yarn run cy:run",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "ci:test": "jest --coverage --config './jest.config.js'"
  },
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.41",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.0",
    "@pact-foundation/pact": "^9.6.0",
    "axios": "^0.19.0",
    "clsx": "^1.1.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.0",
    "date-fns": "^2.9.0",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "formik": "^2.1.2",
    "material-ui-image": "^3.2.2",
    "minify-css-string": "^1.0.0",
    "msal": "^1.2.1",
    "react": "^16.12.0",
    "react-aad-msal": "^2.3.3",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.2.1",
    "react-image-lightbox": "^5.1.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^7.0.4",
    "regenerator-runtime": "^0.13.3",
    "uuid": "^3.4.0",
    "yup": "^0.28.1",
    "zustand": "^2.2.3"
  }
}
